pipeline {
    agent node {
        label 'maven'
    }

    environment {
        PATH = "/opt/apache-maven-3.9.11/bin:$PATH"
    }

    stages {
        stage('clone the project on build server') {
            steps {
                git branch: 'main', url: 'https://github.com/Ranjeetd26/tweet-trend-new.git'
            }
        }
    }
}
